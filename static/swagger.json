{
  "openapi": "3.0.0",
  "info": {
    "version": "1.0.0",
    "title": "Documentation API FoodWise",
    "description":"Foodwise API is designed to provide access to the Foodwise application functionalities. It allows users to register, log in, and predict food ingredients based on images."
  },
  "servers": [
    {
      "url": "http://localhost:5000"
    }
  ],
  "tags": [
    {
      "name": "predict",
      "description": "You can Detect Image by base64 to get rekomendation recep",
      "externalDocs": {
        "description": "Find out more",
        "url": "http://swagger.io"
      }
    },
    {
      "name": "user",
      "description": "Operations about user"
    }
  ],
  "paths": {
    "/register": {
      "post": {
        "tags": [
          "user"
        ],
        "summary": "Create user",
        "description": "The user create acoount",
        "operationId": "createUser",
        "requestBody": {
          "description": "Created user object",
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/register"
              }
            },
            "application/x-www-form-urlencoded": {
              "schema": {
                "$ref": "#/components/schemas/register"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Registrasi berhasil",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/register"
                }
              }
            }
          },
          "400":{
            "description": "Username sudah terdaftar or Email, username, dan password diperlukan",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/register"
                }
              }
            }
          },
          "500":{
            "description": "Internal Server Error"
          }
        }
      }
    },
    "/predict": {
      "post": {
        "tags": [
          "predict"
        ],
        "summary": "Image Detect",
        "description": "User send the image by base64",
        "operationId": "Detect Image",
        "security": [{"jwt": {
          "type": "http",
          "scheme": "bearer",
          "bearerFormat": "JWT",
          "x-bearerInfoFunc": "app.decode_token"
      }}],
        "requestBody": {
          "description": "Predict Image by base64",
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/predict"
              }
            },
            "application/x-www-form-urlencoded": {
              "schema": {
                "$ref": "#/components/schemas/predict"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Login Success",
            "content": {
              "application/json": {
                "schema": {
                  "type": "string"
                }
              }
            }
          },
          "400": {
            "description": "Invalid username/password supplied"
          }
        }
      }
    },
    "/login": {
      "post": {
        "tags": [
          "user"
        ],
        "summary": "Logs user into the system",
        "description": "User do Login",
        "operationId": "loginUser",
        "requestBody": {
          "description": "Login User",
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/login"
              }
            },
            "application/x-www-form-urlencoded": {
              "schema": {
                "$ref": "#/components/schemas/login"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Login Success",
            "content": {
              "application/json": {
                "schema": {
                  "type": "string"
                }
              }
            }
          },
          "400": {
            "description": "Invalid username/password supplied"
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "register": {
        "type": "object",
        "properties": {
          "email": {
            "type": "string",
            "example": "john@email.com"
          },
          "username": {
            "type": "string",
            "example": "theUser"
          },
          "password": {
            "type": "string",
            "example": "12345"
          }
        },
        "xml": {
          "name": "user"
        }
      },
      "login": {
        "type": "object",
        "properties": {
          "username": {
            "type": "string",
            "example": "theUser"
          },
          "password": {
            "type": "string",
            "example": "12345"
          }
        },
        "xml": {
          "name": "user"
        }
      },
      "predict": {
        "type": "object",
        "properties": {
          "base64": {
            "type": "varchar",
            "example": ""
          }
        },
        "xml": {
          "name": "user"
        }
      }
    },
    "securitySchemes": {
      "jwt": {
          "type": "http",
          "scheme": "bearer",
          "bearerFormat": "JWT",
          "x-bearerInfoFunc": "app.decode_token"
      }
    }
  }

}